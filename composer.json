{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "astrotomic/laravel-translatable": "^11.12",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "laracasts/flash": "^3.2",
        "laraeast/laravel-bootstrap-forms": "^5.4",
        "laraeast/laravel-settings": "^1.3",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "spatie/laravel-medialibrary": "^10.11",
        "spatie/laravel-permission": "^5.10",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "spatie/laravel-ignition": "^2.2",
        "laravel/sail": "^v1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^v7.8",
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeds"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-complete:generate": [
            "@php artisan ide-helper:meta --ansi --quiet",
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:models --nowrite --quiet"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:meta --ansi --quiet"
        ],
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
